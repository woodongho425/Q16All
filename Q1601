import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner stdIn = new Scanner(System.in);
    System.out.print("x값 : ");
    int x = stdIn.nextInt();
    System.out.print("x값 : ");
    int y = stdIn.nextInt();
    System.out.println("x * y = "+(x*y));
    System.out.print("x / y = "+(x/y));
  }
}

// 런타임 오류
// 1. 입력 부적합 2. 산술 연산 이외
